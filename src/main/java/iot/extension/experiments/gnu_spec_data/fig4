reset
set xlabel 'Number of stations' font 'Times,12'
set ylabel 'Number of virtual machines' font 'Times,12'
set style data histograms
set style histogram cluster gap 1
set boxwidth 0.7 relative
set yrange[0:14]
set grid ytics
set style fill pattern 1.2 border -1
set xtic 1 offset character 0,0 font 'Times,12'
plot 'fig4data' using 2:xtic(1) ti "interval 1min" lc rgb "#FF0000",\
'' using 3 ti "interval 5min" lc rgb "#0000FF"
